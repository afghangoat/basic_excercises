10 GR
20 X=20: Y=20: T=1
30 C=INT(RND(1)*16)
40 COLOR=C: PLOT X,Y
41 COLOR=C: PLOT Y,X
45 T=T+0.1
50 IF RND(1)>=.9 THEN C=INT(ABS(SIN(T)*COS(T))*16)
60 X = X + INT(TAN(1)*3)*COS(T)*.1 - 1
70 Y = Y + INT(TAN(1)*3)*SIN(T)*.1 - 1
80 IF X > 39 THEN X=0: T=T+40
90 IF X < 0 THEN X=39: T=T+40
100 IF Y > 39 THEN Y=0: T=1
110 IF Y < 0 THEN Y=39: T=1
120 GOTO 40